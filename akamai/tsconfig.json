{
    "compilerOptions": {
      "module": "ES2020",
      "target": "ES2020",
      "sourceMap": true,
      "strict": false,
      "lib": ["es2020", "WebWorker"],
      "outDir": "./out",
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "declaration": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "typeRoots": [
        "./node_modules/@types",
        "./types"
      ],
      "types": ["@types/akamai-edgeworkers", "@types/node"]
    },
    "include": ["**/*.d.ts", "**/*.ts", "**/*.tsx", "../common.ts"],
    "exclude": [
      "node_modules"
    ]
  }


